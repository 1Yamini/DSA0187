#include <iostream>
using namespace std;
int main()
{
    int a[10],i,n;
    cout<<"enter no of elements: ";
    cin>>n;
    cout<<"enter the elements: ";
    for(i=1;i<=n;i++)
    {
        cin>>a[i];
    }
    cout<<"the elements: ";
    for(i=1;i<=n;i++)
    {
        cout<<"\n"<<a[i];
    }
    
}
