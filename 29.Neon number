#include <iostream>
using namespace std;
int main()
{
    int a,r,s=0,s1=0;
    cout<<"enter the number: ";
    cin>>a;
    s=a*a;
    while(a!=0)
    {
        r=s%10;
        s1=s1+r;
        s=s/10;
    }
    if(s1==a)
    {
        cout<<"neon number";
    }
    else
    {
        cout<<"not a neon number";
    }
    
}
